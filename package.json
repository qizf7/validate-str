    "babel-loader": "^7.1.5",
    "babel-preset-env": "^1.7.0",
    "babel-register": "^6.26.0",
    "chai": "^4.1.2",
    "mocha": "^5.2.0",
    "webpack": "^4.16.2",
    "webpack-cli": "^3.1.0",
    "yargs": "^12.0.1"
